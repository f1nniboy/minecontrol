{
    "name": "minecontrol",
    "version": "1.0.6",
    "description": "A Minecraft terminal client that can connect to servers - cracked or premium.",
    "main": "build/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/f1nniboy/minecontrol"
    },
    "scripts": {
